{
  "compilerOptions": {
    "noEmitOnError": false,
    "noEmitHelpers": true,
    "sourceMap": false,
    "removeComments": true,
    "declaration": false,
    "experimentalDecorators": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "target": "ES2017",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "rootDir": ".",
    "baseUrl": ".",
    "plugins": [{
      "transform": "@nativescript/webpack/transformers/ns-transform-native-classes",
      "type": "raw"
    }],
    "paths": {
      "*": ["./node_modules/*"]
    }
  },
  "exclude": ["node_modules"]
}
